{
  "name": "main-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.build.tsbuildinfo tsconfig.tsbuildinfo",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:ci": "eslint \"src/**/*.ts\" --max-warnings 0",
    "prettier": "prettier --write .",
    "mikro-orm": "NODE_ENV=development MIKRO_ORM_CLI_USE_TS_NODE=true mikro-orm",
    "cross:start": "cross-env NODE_ENV=development nest start",
    "cross:start:dev": "cross-env NODE_ENV=development nest start --watch",
    "cross:mikro-orm": "cross-env NODE_ENV=development MIKRO_ORM_CLI_USE_TS_NODE=true mikro-orm",
    "script:trustpositif": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register scripts/fetch-trustpositif-data.ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "@mikro-orm/cli": "^5.6.9",
    "@mikro-orm/core": "^5.6.9",
    "@mikro-orm/migrations": "^5.6.9",
    "@mikro-orm/nestjs": "^5.1.5",
    "@mikro-orm/postgresql": "^5.6.9",
    "@mikro-orm/reflection": "^5.6.9",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^6.3.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv-flow": "^3.2.0",
    "env-var": "^7.3.0",
    "fast-xml-parser": "^5.3.4",
    "helmet": "^6.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "mikro-orm": {
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/src/mikro-orm.config.js"
    ]
  }
}
